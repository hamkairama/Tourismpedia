@model Persada.Fr.ModelView.TOURIS_TV_SOSMED

@{
    ViewBag.Title = "Delete Sosmed";
}

<h2>@ViewBag.Title.</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Type
        </dt>

        <dd>
            @Model.TYPE
        </dd>

        <dt>
            Name
        </dt>

        <dd>
            @Model.NAME
        </dd>

        <dt>
            Description
        </dt>

        <dd>
            @Model.DESCRIPTION
        </dd>

        <dt>
            Icon
        </dt>

        <dd>
            @Model.ICON
        </dd>

        <dt>
            Url
